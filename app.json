{
  "expo": {
    "name": "KartApp",
    "slug": "KartApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon .png",
    "userInterfaceStyle": "automatic",
    "scheme": "kartapp",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "web": {
      "bundler": "metro"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kartapp",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": null
        }
      },
      "googleServicesFile": "./google-services.json"
    },
    "ios": {
      "bundleIdentifier": "com.kartapp",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Este aplicativo precisa acessar sua localização para fornecer recursos baseados em localização.",
        "NSLocationAlwaysUsageDescription": "Este aplicativo precisa acessar sua localização em segundo plano."
      }
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Permitir que o KartApp use sua localização."
        }
      ],
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "newArchEnabled": true
          },
          "ios": {
            "newArchEnabled": true
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "875b4a06-39f4-4bdc-a5be-87de2a9249ca"
      }
    }
  }
}